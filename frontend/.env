# API Configuration, 포트번호만 backend/.env 에서 확보한 값으로 변경
VITE_API_URL=http://backend:3000
VITE_API_PREFIX=/api

# Development Server Configuration
VITE_DEV_SERVER_PORT=5173
VITE_DEV_SERVER_HOST=0.0.0.0

# Production Server Configuration
VITE_PREVIEW_SERVER_PORT=3000

# Security Configuration
VITE_CSRF_TOKEN_HEADER=X-CSRF-Token
VITE_AUTH_TOKEN_HEADER=Authorization

# Application Configuration
VITE_APP_NAME=MyType
VITE_APP_VERSION=1.0.0

# Build Configuration
VITE_BUILD_OUT_DIR=dist
VITE_BUILD_ASSETS_DIR=assets


